{
  "name": "intern",
  "version": "5.0.0-pre",
  "description": "Intern. A next-generation code testing stack for JavaScript.",
  "homepage": "http://theintern.io/",
  "license": "BSD-3-Clause",
  "bugs": "https://github.com/theintern/intern/issues",
  "keywords": [
    "bdd",
    "browserstack",
    "ci",
    "code coverage",
    "continuous integration",
    "javascript",
    "sauce labs",
    "selenium",
    "tdd",
    "test",
    "testing",
    "unit",
    "webdriver"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/theintern/intern.git"
  },
  "bin": {
    "intern": "./bin/intern.js"
  },
  "scripts": {
    "clean": "ts-node scripts/clean.ts",
    "prebuild": "npm run clean",
    "build:src": "ts-node scripts/build.ts",
    "build:api": "ts-node scripts/api.ts",
    "build": "concurrently npm:build:src npm:build:api",
    "devpack": "npm pack ./_build/src",
    "lint": "ts-node scripts/lint.ts",
    "release": "cross-env INTERN_BUILD=release ts-node scripts/release.ts",
    "start": "cross-env ts-node src/bin/intern.ts serveOnly",
    "test": "ts-node scripts/test.ts",
    "preci": "npm run build:src",
    "ci": "npm test config=@ci",
    "watch": "npm run build:src watch",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@types/benchmark": "1.0.31",
    "@types/chai": "4.1.7",
    "@types/charm": "1.0.1",
    "@types/express": "~4.17.0",
    "@types/istanbul-lib-coverage": "~2.0.1",
    "@types/istanbul-lib-instrument": "~1.7.3",
    "@types/istanbul-lib-report": "~1.1.1",
    "@types/istanbul-lib-source-maps": "~1.2.2",
    "@types/istanbul-reports": "~1.1.1",
    "@types/ws": "6.0.1",
    "axios": "~0.19.0",
    "benchmark": "~2.1.4",
    "body-parser": "~1.19.0",
    "chai": "~4.2.0",
    "charm": "~1.0.2",
    "chokidar": "~3.3.0",
    "command-exists": "~1.2.8",
    "commander": "~4.0.1",
    "concurrent": "~0.3.2",
    "decompress": "~4.2.0",
    "diff": "~4.0.1",
    "express": "~4.17.1",
    "glob": "~7.1.4",
    "http-errors": "~1.7.2",
    "istanbul-lib-coverage": "~2.0.5",
    "istanbul-lib-hook": "~2.0.7",
    "istanbul-lib-instrument": "~3.3.0",
    "istanbul-lib-report": "~2.0.8",
    "istanbul-lib-source-maps": "~3.0.6",
    "istanbul-reports": "~2.2.6",
    "jszip": "~3.2.2",
    "lodash": "~4.17.15",
    "mime-types": "~2.1.24",
    "minimatch": "~3.0.4",
    "open": "~7.0.0",
    "platform": "~1.3.5",
    "qs": "~6.9.1",
    "resolve": "~1.11.1",
    "shell-quote": "~1.6.1",
    "source-map": "~0.6.1",
    "ts-node": "~8.2.0",
    "tslib": "~1.10.0",
    "ws": "~7.0.0"
  },
  "devDependencies": {
    "@dojo/loader": "^2.0.0",
    "@types/body-parser": "^1.17.0",
    "@types/command-exists": "~1.2.0",
    "@types/decompress": "~4.2.3",
    "@types/diff": "^4.0.2",
    "@types/glob": "^7.1.1",
    "@types/grunt": "^0.4.25",
    "@types/http-errors": "^1.6.1",
    "@types/istanbul-lib-hook": "^2.0.0",
    "@types/jsdom": "^12.2.3",
    "@types/jszip": "~3.1.6",
    "@types/lodash": "^4.14.141",
    "@types/mime-types": "^2.1.0",
    "@types/node": "^12.0.6",
    "@types/parse5": "^5.0.0",
    "@types/platform": "^1.3.2",
    "@types/qs": "~6.9.0",
    "@types/resolve": "^0.0.8",
    "@types/semver": "^6.2.0",
    "@types/shell-quote": "^1.6.1",
    "@types/shelljs": "^0.8.5",
    "@types/sinon": "^7.0.12",
    "@types/systemjs": "^0.20.2",
    "@types/webpack": "^4.4.32",
    "ajv": "^6.10.0",
    "concurrently": "^3.5.0",
    "core-js": "^3.1.3",
    "core-js-pure": "^3.1.3",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "jsdom": "^15.1.1",
    "lint-staged": "^8.2.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.17.1",
    "semver": "^6.3.0",
    "shelljs": "^0.8.3",
    "sinon": "^7.3.2",
    "source-map-support": "^0.5.12",
    "style-loader": "^0.23.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "ts-loader": "^6.0.2",
    "tsconfig-paths": "~3.9.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.15.3",
    "url-search-params-polyfill": "^6.0.0",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.2"
  },
  "lint-staged": {
    "src/**/*.{ts,json,css}": [
      "prettier --write",
      "git add"
    ],
    "tests/**/*.{ts,json,css}": [
      "prettier --write",
      "git add"
    ],
    "*.ts": [
      "prettier --write",
      "git add"
    ],
    "{intern,tsconfig}.json": [
      "prettier --write",
      "git add"
    ]
  },
  "pre-commit": "precommit",
  "prettier": {
    "useTabs": false,
    "singleQuote": true,
    "tabWidth": 2,
    "proseWrap": "always"
  }
}
