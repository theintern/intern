{
	"compilerOptions": {
		"baseUrl": "..",
		"declaration": false,
		"outDir": "../_tests",
		"paths": {
			"src/*": [
				"_build/src/*",
				"src/*"
			]
		}
	},
	"extends": "../tsconfig.json",
	"include": [
		"./globals.d.ts",
		"./benchmark/example.ts",
		"./support/**/*.ts",
		"./unit/intern.ts",
		"./unit/bin/*.ts",
		"./unit/lib/browser/*.ts",
		"./unit/lib/channels/*.ts",
		"./unit/lib/common/*.ts",
		"./unit/lib/executors/*.ts",
		"./unit/lib/interfaces/bdd.ts",
