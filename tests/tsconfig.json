		"./unit/lib/reporters/Coverage.ts",
		"./unit/lib/reporters/HtmlCoverage.ts",
		"./unit/lib/reporters/JsonCoverage.ts",
		"./unit/lib/reporters/JUnit.ts",
		"./unit/lib/reporters/Lcov.ts",
		"./unit/lib/reporters/Reporter.ts",
		"./unit/lib/reporters/Runner.ts",
		"./unit/lib/reporters/TeamCity.ts",
		"./unit/lib/*.ts",
